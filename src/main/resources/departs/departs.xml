<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="listDeparts" extends="json-default"
		namespace="/">
		<action name="listDeparts"
			class="com.struts.controller.DepartsActionController"
			method="listDeparts">
			<result name="success" type="json"></result>
		</action>
	</package>
	<package name="addDeparts" extends="struts" namespace="/">
	<action name="addDeparts" class="com.struts.controller.DepartsActionController" method="addDeparts">
	</action>
	</package>
</struts>